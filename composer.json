	"keywords": ["wordpress", "plugin", "zerospam", "spam"],
	"license": "GPL-2.0-or-later",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name": "Ben Marshall",
			"email": "me@benmarshall.me",
			"homepage": "https://www.benmarshall.me"
		},
		{
			"name": "Highfivery",
			"email": "info@highfivery.com",
			"homepage": "https://highfivery.com"
		}
